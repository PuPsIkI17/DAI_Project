{
    "version": 2,
    "alias": "meal-planner",
    "name": "meal-planner",
    "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "^/favicon.ico$",
            "dest": "/favicon.ico"
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }
    ]
}